{
  "name": "tiny-bilibili-ws",
  "type": "module",
  "version": "0.0.7",
  "packageManager": "pnpm@7.5.2",
  "description": "Bilibili Live WebSocket/TCP API",
  "author": "starknt",
  "license": "MIT",
  "homepage": "https://github.com/starknt/tiny-bilibili-ws#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/starknt/tiny-bilibili-ws.git"
  },
  "bugs": "https://github.com/starknt/tiny-bilibili-ws/issues",
  "keywords": [
    "ws",
    "websocket",
    "bilibili",
    "live"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "require": "./dist/browser.cjs",
      "import": "./dist/browser.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "browser.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm publish",
    "dev": "unbuild --stub",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "pako": "^2.1.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.6",
    "@types/node": "^20.2.1",
    "@types/pako": "^2.0.0",
    "@types/ws": "^8.5.4",
    "bumpp": "^9.1.0",
    "eslint": "^8.40.0",
    "esno": "^0.16.3",
    "pnpm": "^8.5.1",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "vitest": "^0.31.1"
  }
}
